<tr>
  <th><%= task.title %></th>
  <td>
    <ul>
      <% task.tags.each do |tag| %>
        <li>
          <p><%= tag.name %></p>
        </li>
      <% end %>
    </ul>
  </td>
  <td>
    <%= link_to "Delete", task, method: :delete,
      data: { confirm: "You sure?" }, class: "btn btn-danger" %>
  </td>
</tr>